@article{gonzalez_martinez_correos_2015,
	title = {Correos y comunicación escrita en la {América} colonial: esquemas de distribución de la correspondencia oficial (1511-1768)},
	volume = {52},
	issn = {1438-4752},
	doi = {10.7767/jbla-2015-0104},
	abstract = {During the second half of the eighteenth century, the Spanish American mail apparatus began to be administered, directly, by the Spanish Crown. Until that time, the mail apparatus was administrated under concessions granted to particular individuals (called Correos Mayores). Indeed, in 1514 Lorenzo Galindez Carvajal received the title of Correo Mayor of the Indies but he exercised this right only partially. The central hypothesis of the article suggests that the colonial mail was not organized in the same way in all Spanish possessions. Instead, the mail apparatus functioned combining different administrative strategies depending on the practical necessities of each viceroyalty. Although the Crown tried in principle to organize the mail into a single body of egulations, the most important viceroyalties and Audiencias distributed the written correspondence in a differentiated manner. The following sections identify the three main models of mail administration in Peru, New Spain and New Kingdom of Granada. In general, the Spanish American mail apparatus was perceived by colonial officials as flexible but perfectible subject to modification to address their particular needs.},
	language = {Spanish},
	number = {1},
	journal = {Jahrbuch für Geschichte Lateinamerikas, Anuario de Historia de America Latina},
	author = {González Martínez, N.F.},
	year = {2015},
	keywords = {Peru, Correo Mayor, Colonial Latin America, Nueva Granada, Nueva España},
	pages = {37--64},
}

@article{martinez_communicating_2021,
	title = {Communicating an {Empire} and {Its} {Many} {Worlds}: {Spanish} {American} {Mail}, {Logistics}, and {Postal} {Agents}, 1492-1620},
	volume = {101},
	issn = {0018-2168},
	doi = {10.1215/00182168-9366571},
	abstract = {This article examines the principles underlying Spanish American mail during the government of the first Hapsburgs. I propose that this mail system, in which official and unofficial postal services coexisted, allowed for an intense communicational experience; rather than restricting correspondence, mail circulated at unprecedented levels. To understand this system's rationale I focus on the figure of the correos mayores, who were responsible for the distribution of official information (or information of interest to the crown) within certain Spanish American cities. Using sources in American and European archives, I question the premise that Spanish American communication was chaotic during this period. I also argue that the exceptional circulation of mail within Spanish America and overseas during the sixteenth century is essential for understanding European expansion and the early modern world.},
	language = {English},
	number = {4},
	journal = {HAHR},
	author = {Martínez, N.F.G.},
	year = {2021},
	keywords = {Postal Service, Spain, Correo Mayor, Colonial Latin America},
	pages = {567--596},
}

@article{sellers-garcia_mail_2012,
	title = {The {Mail} in {Time}: {Postal} {Routes} and {Conceptions} of {Distance} in {Colonial} {Guatemala}},
	volume = {21},
	issn = {1060-9164},
	doi = {http://dx.doi.org/10.1080/10609164.2012.661976},
	abstract = {In the late eighteenth century, Bourbon administrators of the postal system in Spain wrote to their subordinates in the Americas, asking for comprehensive topographic maps of the mail and post system, which had been steadily improved and expanded beginning in the 1760s. As part of the larger Bourbon reforms, the innovations to the mail system promised to shrink the vast Spanish empire, quickening communication, strengthening the bureaucratic chain of command, and drawing distant peripheries like Guatemala closer to the center. The administrators expected in reply, no doubt, aerial cartographic depictions of the kind then being produced in Spain: maps that would detail the breadth of the American postal systems with precise routes, placenames, and distances. In some places, certainly, such postal maps could be successfully produced. An 1804 document sent to administrators in Spain from Buenos Aires, for example, offers a detailed cartographic representation of the local ‘mail and postal offices,’ quantifying the distances along the mail routes from place to place. Place names and distances are identified alongside topographical features, such as the network of rivers and tributaries fanning out from Rı́o de la Plata. Spreading across the surface of the map like veins, the detailed waterways appear to dominate the landscape, filling the map with their prominent titles. But the map from Buenos Aires was not the universal representational model during the Bourbon era. Officials elsewhere chose other textual and visual forms to report on the spatial reach of their respective rentas. A Chilean ‘Table of the distances that exist between the cities and other principal places in the Kingdom of Chile, based on the most exact calculation possible according to estimates,’ dated 1772, places the quantification of distance at the axis of two place-names. Similar to a modern chart calculating driving distances, the Chilean chart maximizes the use of space on the page to quantify the distances between more than forty places.},
	language = {English},
	number = {1},
	journal = {Colonial Latin American Review},
	author = {Sellers-García, S.},
	year = {2012},
	keywords = {Guatemala, Pre-philately, Postal Routes, Colonial Latin America},
	pages = {77--99},
}

@article{stangl_correos_2020,
	title = {Los correos terrestres de {Cartagena} de {Indias} en tiempos de la renta (1768-1810): {Itinerarios}, cartografía, un «mapa en relación topográfica», {GPS} y un {SIG}},
	volume = {80},
	issn = {0034-8341},
	doi = {10.3989/revindias.2020.007},
	abstract = {This article attempts a reconstruction of the spatial organization of land mail in the area of ​​the main post office in Cartagena, installed in 1768. It is essentially based on two handwritten reports, one by José Antonio de Pando from approximately 1774 and another anonymous. (probably by Antonio de Miranda) from 1801. Both documents contain detailed information on the frequency of the races, the places of transit, the distances between them and the quality of the roads. Using complementary information such as current roads, GPS data and georeferenced maps of the time, the routes are reconstructed with a geographic information system (GIS) in a process that at the same time allows considering the practices of measuring and expressing distances in that weather.},
	language = {Spanish},
	number = {278},
	journal = {RDI},
	author = {Stangl, W.},
	year = {2020},
	keywords = {Peru, Chile, Colombia, Panama, Pre-philately, Postal Routes, Cartagena, Colonial Latin America, Rio del la Plata, Yndias, GIS, GPS, Tierra Firme},
	pages = {199--250},
}

