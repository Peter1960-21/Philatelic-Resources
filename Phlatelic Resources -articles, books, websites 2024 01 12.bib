@article{braga_brasilia_2021,
	title = {Brasília nos {Inteiros} {Postais}: estudos dos aerogramas e bilhetes postais {Brasileiros}},
	volume = {24},
	issn = {2527-2675},
	url = {https://revistarebram.com/index.php/revistauniara/article/view/793},
	abstract = {Apesar da existência de trabalhos sobre as emissões postais de selos adesivos relacionados a Brasília, ainda não se tinha nenhum estudo sistemático sobre as emissões de inteiros postais oficiais também alusivas a Brasília, apesar da importância filatélica e postal dessas peças. Nesse trabalho foi, através de um levantamento sistemático em aerogramas e bilhetes postais oficiais brasileiros, realizado a identificação dessas emissões cuja temática Brasília está diretamente presente, independente de sua manifestação se der em primeiro ou em segundo plano. Foram identificados 12 inteiros postais oficiais brasileiros, sendo 7 aerogramas e 5 bilhetes postais. Cronologicamente o primeiro inteiro postal identificado como alusivo a Brasília foi o bilhete postal emitido em 1970 em comemoração ao VIII Congresso Eucarístico Nacional. O ícone imagético relacionado à Brasília mais encontrado nos inteiros postais foi o da Catedral Metropolitana Nossa Senhora Aparecida, seguido pelo Congresso Nacional e pela imagem do presidente JK.},
	language = {Portuguese Brazilian},
	number = {2},
	urldate = {2022-06-16},
	journal = {ReBraM},
	author = {Braga, H.C.},
	month = sep,
	year = {2021},
	keywords = {Brasil, Postal Stationery, Aerogramme},
	pages = {191--206},
}

