}

@article{aracil_vuelo_2002,
	title = {El vuelo del {Zeppelin} a {América} del {Sur} de 1930},
	volume = {3},
	issn = {1577-0842},
	url = {https://www.rahf.es/revista-academvs/},
	abstract = {The author recounts the 1930 flight of the Zeppelin airship. It lasted from May 18 to June 6, following the route Friedrichshafen, Sevilla, Pernambuco, Río de Janeiro, Pernambuco, Lakehurst, Sevilla, Friedrichshafen. A total of 22 passengers were on board. A detailed account of the schedules and stopovers is provided. Also, records of the foreign mail delivered in Spain and sent from Spain to both American and European destinations are detailed. He also delves into the regulations and notices concerning this flight and the mail it carried.},
	language = {Spanish},
