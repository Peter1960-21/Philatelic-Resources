@misc{botero_colombia_nodate,
	title = {Colombia: {Classic} {Colombia} {The} {First} {Six} {Issues} 1859 to 1866},
	copyright = {Museum of Philately},
	url = {https://www.museumofphilately.com/collection/35},
	abstract = {An important collection covering the scarcest period of stamp issues in Colombia, and representing undoubtedly one of the best, if not the best ever formed of classic Colombia. The Exhibit is titled "Classic Colombia: The First Six Issues - 1859 to 1866' and is an internationally recognised FIP Large Gold medal winning collection at New York in 2015 and Brasilia in 2017. The exhibit includes essays and proofs, plate studies, shades, multiples, a wide range of cancellations, rare usages of early Colombian issues on cover, as well as the exceedingly scarce and sought after inverted clichés and clichés erroneously placed in their non-corresponding lithographic stones (transfer errors). Many of the rarities assembled in this collection have prestigious provenance, including pieces owned by Ferrary, Caspary, Burrus, Newbury, Neuburger, Jewell, Hubbard, Larsen, Goeggel, Kaplan, Londoño, Frohlich and Santamaría. It undoubtedly represents the largest number of important covers and multiples ever gathered in one Columbia exhibit. Formed by Ricardo Botero over a period of more than 50 years, and includes the finest examples known of Classic Colombia.},
	author = {Botero, R.},
	keywords = {Colombia, Classic, Exhibit},
}

