@book{bongiovanni_treinta_2013,
	address = {Quito},
	title = {Los treinta meses de la {SCADTA} en {Ecuador} [{The} {Thirty} {Months} of {SCADTA} in {Ecuador}]},
	abstract = {Dr. Bongiovanni reveals to a number of misconceptions about these stamps. He tells us of the manipulations by the SCADTA representative to "create" varieties for the philatelic market and to fictitiously reduce in some cases the amounts issued as is the case of "50 Cts." error.Some of the highlights of this book are:1. The "50 Cts." error was supposedly issued in a quinatity of 100 (4 sheets of 25) subsequently anothe source speaks of 250 (10 sheets of 25). Neither of these reflect reality; many more appeared on the market from the very begining suggesting that there were more than one printing of this "error".2. No filatelic catalog or any other source mentions the "75 Cts." error prior 1937. In fact, Wilfrido Moreno, the producer of the surcharge, in a statement declared under oath that never produced the "75 Cts." error surcharge. It was neither, produced or used for franking during the period SCADTA operated in Ecuador. Since the stamp was privately produced it can therefore only by considered label.As for prices, the "50 Cts." error, is relatively abundant and the price assigned to it by the catalogues is mucho too high. Not so if the stamp is found on cover or used; then it deserves a premium. As for the "75 Cts." error as much as \$ 8,000 have been paid for this label.},
	language = {English, Spanish},
	author = {Bongiovanni, I.},
	year = {2013},
	keywords = {Ecuador, Overprint, SCADTA, Airmail, Error},
}

