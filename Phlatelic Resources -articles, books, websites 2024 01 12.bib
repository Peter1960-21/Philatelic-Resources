@article{reinhardt_introduction_1997,
	title = {The {Introduction} of {Aniline} {Dyes} to {Paper} {Printing} and {Queen} {Victoria}'s {Postage} {Stamps}},
	volume = {44},
	issn = {0002-6980},
	url = {https://doi.org/10.1179/amb.1997.44.1.11},
	doi = {10.1179/amb.1997.44.1.11},
	abstract = {The synthetic dye industry emerged during 1858-60, following the discovery of Perkin's mauve (aniline purple), and Verguin's aniline red (magenta, or fuchsine).l The introduction of these aniline colours posed a special problem in that they did not readily take to cotton fabrics. The problem had, to a considerable extent, been overcome during the early 1860s, as was demonstrated in the displays of dyes and dyed and printed goods at the London
International Exhibition of 1862. Barely had the European synthetic dye industry become established, however, when it was threatened by events across the Atlantic Ocean. The American CivilWar precipitated a cotton famine that was to last from 1862 until 1866, a famine that severely reduced the output of major centres of cotton, or calico, printing. No region was hurt more than Lancashire. In an effort to find a new market for aniline dyes, the leading Lancashire dye manufacturer, Roberts, Dale \& Co., of Manchester, began to
investigate openings in paper printing. The difficulties confronted in this application were to prove no less formidable than for textile printing. Indeed, in some respects they were more problematic since, unlike the situation for textile dyeing and printing, dye-manufacturers had no specialist knowledge of paper printing. That the problems were overcome by the mid 1860s was due in no small part to the efforts of the chemists Heinrich Caro, at Roberts, Dale \& Co., and Hugo Muller, at the London printing company of Thomas De La Rue \& Co. Perhaps the most novel application of synthetic dyes that arose from their working together was in security printing, especially of postage stamps.
This encouraged the conversion of dyes into high quality lakes and pigments.},
	number = {1},
	journal = {Ambix},
	author = {Reinhardt, C. and Travis, A.S.},
	month = mar,
	year = {1997},
	note = {Publisher: Taylor \& Francis},
	keywords = {Printing, United Kingdom, Philatelic Research, Research, Pigments},
	pages = {11--18},
	annote = {doi: 10.1179/amb.1997.44.1.11},
}

@article{simonetti_has_2016,
