@article{benedict_nejd_2006,
	title = {The {Nejd} {Surcharges}},
	volume = {69},
	abstract = {A shortage of stamps necessitated the surcharging of stamps with the “Nejd Sultanate Post” handstamp in blue and black. The 1pi and 1-1/2pi surcharges were placed on the 1/2pi stamp, while the 2pi surcharge was put on the 3pi stamp.

