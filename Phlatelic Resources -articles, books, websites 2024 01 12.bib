@article{harris_ecuador_1965,
	title = {Ecuador; {Postal} services of {Spain} and the {Indies}, with particular reference to {Ecuador}},
	issn = {0032-5341},
	abstract = {There are at least three ways to formulate a postal history commentary. The first is through consideration of covers or similiar material of an obvious philatelic nature. The second is through analysis of contemporary holographic documents bearing upon the subject. These two approaches have been previously utilized by the author in his attempt to more fully document the postal history of the area which now is the Republic of Ecuador.1 The final way, and the one treated in this article, is through an analysis of certain old laws
and collateral historical material bearing upon the postal services in and to the area now comprising the Republic of Ecuador, from earliest Colonial times, through 1864. It is promised that, with the completion of this trilogy of articles, the author will not again allow type to be set to paper upon this obviously introverted subject.
The material for consideration in this article includes volumes of laws promulgated by the various Monarchs of Spain, beginning in the Kith Century; certain historical reports bearing upon the colonial mail services within the Audiencia of Quito, the territorial forerunner of Ecuador; and finally, laws of the Ecuadorean Congress, and decrees and circular instructions of the Presidents of Ecuador, from 1830, the date Ecuador achieved its sole independence, to the end of 1864, when the first adhesive postage stamps were contracted for.},
	language = {English},
	number = {16},
	journal = {PHJ},
	author = {Harris, L.J.},
	month = jun,
	year = {1965},
	keywords = {Postal Rates, Pre-philately, Ecuador, Cover, Postal Service, Document, Postal Routes, Spain, Spanish Latin America, Spanish Indies},
