@misc{rmpl_denver_great_2020,
	address = {Denver, Colo.},
	title = {The {Great} {Postal} {Fraud} of {Mexico} 1870 - 1872},
	url = {https://www.youtube.com/watch?v=oPSl6XnXWDQ},
	abstract = {An in-depth look at the postal fraud that occurred in Mexico from 1870-1872 and the methods used by the Mexican government to counteract the fraud by means of using Anatado overprints and Habilitados. Presented by Nick Follansbee, Dr. Randall Grace and John Kordich during the Philately of the Americas Seminars at the Rocky Mountain Stamp Show in Denver Colorado in May 2019.

Nick Follansbee describes how postal forgeries came to be in Mexico in 1870-1871 and the effect these forgeries had on the Mexican postal system. Nick describes the means used to produce the forgeries and how the forgeries can be identified.

Dr. Randall Grace describes how Anatado overprints were employed by the Mexican postal authorities to prevent the unauthorized use of the postal forgeries and how this solution fell prey to forgery as well. Dr. Grace discusses in great detail the many ways that Anatado overprint forgeries can be identified.

John Kordich closes the presentation with a discussion of the use of Habilitados to further prevent the unauthorized use of the postal forgeries and how to identify fake Habilitados that began appearing shortly after their introduction by the Mexican postal authorities..},
	language = {English},
	urldate = {2021-09-27},
	publisher = {RMPL Denver},
	author = {{RMPL Denver}},
	collaborator = {Follansbee, N. and Grace, R. and Kordich, J.},
	month = mar,
	year = {2020},
	keywords = {Mexico, Forgery},
}

